{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.0.0prealpha1",
  "default_locale": "en",
  "author": "Achim Seufert",
  "homepage_url": "http://mozext.achimonline.de",
  "applications": {
    "gecko": {
      "id": "nestedquoteremover@mozext.achimonline.de",
      "strict_min_version": "78.0"
    }
